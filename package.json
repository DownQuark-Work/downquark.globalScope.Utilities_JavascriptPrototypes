{
  "name": "@downquark/utils-js-prototypes",
  "version": "0.1.0",
  "description": "DownQuark's commonly used set of javascript functions",
  "main": "src/index.js",
  "scripts": {
    "build": "flow-remove-types --all src/js -d dist/",
    "flow:ls": "yarn flow ls",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install",
    "postinstall": "yarn flow-typed",
    "prepublish": "yarn run build",
    "test:info": "by building first we remove the flow annotations that is needed by tap",
    "test": "yarn run build && tap",
    "test:coverage": "yarn run build && tap --coverage-report=lcov",
    "test:dev:bail": "yarn run build && tap --bail --save=coverage/failures.txt",
    "test:dev": "yarn run build && tap --bail --save=coverage/failures.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DownQuark-Work/downquark.packagedApp.Npm_JavascriptPrototypes.git"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/DownQuark-Work/downquark.packagedApp.Npm_JavascriptPrototypes/issues"
  },
  "homepage": "https://github.com/DownQuark-Work/downquark.packagedApp.Npm_JavascriptPrototypes#readme",
  "devDependencies": {
    "flow": "^0.2.3",
    "flow-bin": "^0.128.0",
    "flow-remove-types": "^2.128.0",
    "rimraf": "^3.0.2",
    "tap": "^14.10.7"
  }
}
